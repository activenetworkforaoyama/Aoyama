jQuery(document).ready(function(){
	var op_button;
	var subItemCode;
	jQuery("#item").change(function(){
		var item = jQuery("#item").val();
		if(item=="PANTS"){
			subItemCode = "03";
			op_button = jQuery("#op_button option:checked").val();
			ByAjaxGetOpPantsOpButtons(subItemCode,op_button);
			jQuery("#op_button").change(function(){
				op_button = jQuery("#op_button option:checked").val();
				ByAjaxGetOpPantsOpButtons(subItemCode,op_button);
			})
			
		}
	})
	
});


function GetOpPantsOpButtons(subItemCode,op_button){
	var op_btnMateStkNo = jQuery("#op_btnMateStkNo");
	op_btnMateStkNo.empty();
	jQuery.ajax({
		 type:"post",
		 url: contextPath + "/order/getOpPantsOpButtons",
		 data:{"itemCode":subItemCode,"opButton":op_button},
		 success:function(result){
			 for (var key in result) {
				 op_btnMateStkNo.append(jQuery('<option />').val(key).text(result[key]));
			 }
		 }
	}); 
}


