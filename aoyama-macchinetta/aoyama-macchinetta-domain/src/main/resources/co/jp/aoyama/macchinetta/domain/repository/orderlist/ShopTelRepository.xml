<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="co.jp.aoyama.macchinetta.domain.repository.orderlist.ShopTelRepository" >
  <resultMap id="BaseResultMap" type="co.jp.aoyama.macchinetta.domain.model.ShopTel" >
    <id column="shop_code" property="shopCode" jdbcType="VARCHAR" />
    <result column="shop_tel" property="shopTel" jdbcType="VARCHAR" />
    <result column="shop_name" property="shopName" jdbcType="VARCHAR" />
    <result column="gyotai" jdbcType="INTEGER" property="gyotai" />
    <result column="created_user_id" property="createdUserId" jdbcType="VARCHAR" />
    <result column="created_at" property="createdAt" jdbcType="TIMESTAMP" />
    <result column="updated_user_id" property="updatedUserId" jdbcType="VARCHAR" />
    <result column="updated_at" property="updatedAt" jdbcType="TIMESTAMP" />
  </resultMap>
  <select id="findShopTel" resultMap="BaseResultMap" parameterType="java.lang.String" >
  		SELECT 
  		shop_code,
  		shop_tel,
  		shop_name,
  		gyotai,
  		created_user_id,
  		created_at,
  		updated_user_id,
  		updated_at
  		FROM mtb_shop_tel
  		order by shop_name
  </select>
</mapper>